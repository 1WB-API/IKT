x = 10
y = 5
z = 3

value1 = (x + y - z) / 5
if y > value1:
    max_value = y
else:
    max_value = value1

value2 = (x + y - z) / y
if 2*x / max_value < value2:
    min_value = 2*x / max_value
else:
    min_value = value2

print(f"x = {x}")
print(f"y = {y}")
print(f"z = {z}")
print(f"u = {min_value}")
