print('ЧАСТЬ 1: ')
import math
#1
x=2
y=2
r1 = (y**(3**x))-(x**3)-(math.exp(-x/3))
print('1) ', r1)

#2
x=2
y=2
r2 = math.exp((x**2)-1)-2*math.log(abs(x+1))-3/(x*y)
print('2) ', r2)

#3
x=2
r3 = 0.8*(math.sin((x/3)-((x+2)/(x+1)))**2)**3
print('3) ', r3)

#4
x=2
y=3
r4 = ((math.cos(3*x**2)**3+x**(1/2))/(x+4*y))-((x+1)/(x-1))**(1/3)
print('4) ', r4)


print('ЧАСТЬ 2: ')
import math
g=3
h=1
x=g+3*h
t=math.cos(2*g-6*h)**2
r = ((math.cos(x)+7)**(1/3)+4)/(5*x+t)
print('Результат: ', r)
